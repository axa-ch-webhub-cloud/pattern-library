{
  "name": "@axa-ch/patterns-library",
  "version": "2.0.1-beta.160",
  "description": "AXA Switzerland Web Patterns",
  "main": "stack/server.js",
  "module": "lib",
  "scripts": {
    "clean": "rimraf dist && rimraf lib && rimraf docs",
    "clean-dev": "rimraf .tmp",
    "test": "npm run test-eslint && npm run test-stylelint",
    "test-editorconfig": "editorconfig-checker src",
    "test-eslint": "eslint src/",
    "test-stylelint": "stylelint 'src/**/*.scss'",
    "toc": "doctoc CONTRIBUTING.md --title '**Table of Contents**'",
    "start": "cross-env NODE_ENV=production npm run serve-build-prod",
    "new": "node stack/tasks/create-a-m-o.js",
    "server": "node stack/server.js",
    "postinstall": "npm rebuild node-sass",
    "watch": "cross-env NODE_ENV=development concurrently \"npm run watch-scss\" \"npm run watch-js\" \"npm run watch-html\" \"npm run watch-svg\"",
    "watch-scss": "npm-watch build-css-dev",
    "watch-js": "node stack/tasks/copy-polyfills.js && node stack/tasks/watch-js.js",
    "watch-html": "npm-watch build-html-dev",
    "watch-svg": "npm-watch build-icons-dev",
    "serve": "concurrently \"npm run server\" \"npm run watch\" \"npm run bundle-demos-dev\"",
    "bundle-demos-dev": "node stack/tasks/bundle-demos.js",
    "bundle-demos-prod": "node stack/tasks/bundle-demos.js",
    "bundle-prod": "concurrently \"node stack/tasks/bundles/all-prod.js\" \"node stack/tasks/copy-polyfills.js\" \"npm run bundle-demos-prod\"",
    "serve-build-prod": "concurrently \"npm run server\" \"npm run build\"",
    "build-css-dev": "concurrently \"node-sass src/index.scss -o .tmp/\" \"node-sass src/sg.scss -o .tmp/\"",
    "build-css-prod": "concurrently \"node-sass src/index.scss -o dist/\" \"node-sass src/sg.scss -o dist/\"",
    "postbuild-css-prod": "postcss dist/index.css -o dist/index.css",
    "build-js-dev": "concurrently \"node stack/tasks/copy-polyfills.js\" \"node stack/tasks/watch-js.js\"",
    "build-html-dev": "concurrently \"node stack/tasks/build.html.js\" \"node stack/tasks/build-examples.js\"",
    "build-js-prod": "npm run bundle-prod",
    "build-icons-dev": "node stack/tasks/icons.js",
    "build-icons-prod": "node stack/tasks/icons.js",
    "prebuild-dev": "npm run clean-dev",
    "build-dev": "concurrently \"npm run build-css-dev\" \"npm run build-html-dev\" \"npm run build-js-dev\" \"npm run build-icons-dev\"",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production concurrently \"node stack/tasks/build.html.js\" \"node stack/tasks/build-examples.js\" \"npm run build-css-prod\" \" npm run build-js-prod\" \"npm run build-lib\" \"npm run build-icons-prod\"",
    "postbuild": "node stack/tasks/copy-dist-to-docs.js",
    "build-lib": "concurrently \"babel src/js --out-dir lib/js --plugins transform-runtime\" \"node stack/tasks/bundles/lib.js\"",
    "release": "node ./stack/tasks/release.js",
    "xprebump-major": "npm run test",
    "bump-major": "npm run test && npmversion --increment major",
    "xprebump-minor": "npm run test",
    "bump-minor": "npm run test && npmversion --increment minor",
    "xprebump-patch": "npm run test",
    "bump-patch": "npm run test && npmversion --increment patch",
    "xprebump-major-beta": "npm run test",
    "bump-major-beta": "npm run test && npmversion --increment major --preid beta --force-preid",
    "xprebump-minor-beta": "npm run test",
    "bump-minor-beta": "npm run test && npmversion --increment minor --preid beta --force-preid",
    "xprebump-patch-beta": "npm run test",
    "bump-patch-beta": "npm run test && npmversion --increment patch --preid beta --force-preid",
    "xprebump-beta": "npm run test",
    "bump-beta": "npm run test && npmversion --increment prerelease --preid beta --force-preid"
  },
  "watch": {
    "build-css-dev": {
      "patterns": [
        "src"
      ],
      "extensions": "scss",
      "quiet": false
    },
    "build-html-dev": {
      "patterns": [
        "src"
      ],
      "extensions": "html",
      "quiet": false
    },
    "build-icons-dev": {
      "patterns": [
        "src"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axa-ch/patterns-library"
  },
  "keywords": [
    "axa"
  ],
  "engines": {
    "node": ">=8.8.1",
    "npm": "~5.4.2"
  },
  "author": "Luca Mele",
  "license": "Copyright 2017 AXA Versicherungen AG",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.4.0",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "cpx": "~1.5.0",
    "cross-env": "^5.1.6",
    "cssnano": "^3.10.0",
    "del": "~3.0.0",
    "doctoc": "^1.3.1",
    "editorconfig-checker": "^1.1.5",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "execa": "^0.10.0",
    "express": "~4.16.2",
    "fkill": "^5.3.0",
    "glob": "^7.1.2",
    "mkdirp": "~0.5.1",
    "node-dir": "^0.1.17",
    "node-sass": "^4.5.3",
    "node-syntaxhighlighter": "^0.8.1",
    "nodemon": "^1.14.11",
    "npm-watch": "^0.3.0",
    "npmversion": "~1.4.0",
    "outdent": "^0.4.1",
    "postcss": "^6.0.14",
    "postcss-cli": "^4.1.1",
    "promise.series": "^0.2.0",
    "readline": "^1.3.0",
    "rimraf": "^2.6.2",
    "rollup": "0.55.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-uglify": "^2.0.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "svg-sprite": "^1.3.7",
    "vinyl": "^2.1.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.18",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "core-js": "~2.5.1",
    "innersvg-polyfill": "0.0.2",
    "nanohtml": "^1.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "react": ">=0.14.0"
  }
}
