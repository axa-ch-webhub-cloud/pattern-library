{
  "name": "@axa-ch/pattern-library",
  "version": "2.0.0",
  "description": "[Check out our Contribution File](https://github.com/axa-ch-webhub-cloud/pattern-library/blob/develop/CONTRIBUTION.md#rules-and-lintings)",
  "module": "lib",
  "scripts": {
    "build": "NODE_ENV=production npm run build-components",
    "watch": "npm run watch-components",
    "build-components": "lerna run build --parallel",
    "watch-components": "lerna run watch --parallel",
    "setup-npm-links": "node scripts/setup-npm-link.js",
    "postinstall": "lerna bootstrap --hoist && cross-env NODE_ENV=production npm run build-components && lerna run --parallel genPackagelock",
    "release": "git clean -dfxq -e .idea -e .vscode && npm whoami && npx lerna clean && npm ci && lerna version --exact && cross-env NODE_ENV=production npm run build-components && lerna publish from-package && git clean -dfxq -e .idea -e .vscode && npm install && git add -A && git commit -m \"release: update package locks\" && git push",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -s src/static",
    "storybook-test": "start-storybook -p 9999 -s src/static --ci",
    "build-storybook": "build-storybook -s ./src/static",
    "test": "npm run test-lint && prettier --check \"./src/**/*\" && node ./scripts/build.test.js && npm run test-jest && npm run test-ui",
    "test-lint": "eslint src/",
    "test-jest": "jest --env=jsdom",
    "test-playwright": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 jest --config=jest.ui.config.js",
    "debug-playwright": "PWDEBUG=1 TEST_HOST_STORYBOOK_URL=http://localhost:6006 jest --config=jest.ui.config.debug.js",
    "test-ui": "sh scripts/ui-test-runner.sh",
    "test-testcafe": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 testcafe chrome ./**/ui.test.js",
    "test-testcafe-headless": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 testcafe 'chrome:headless' ./**/ui.test.js",
    "test:windows": "npm run test-lint && prettier --check \"./src/**/*\" && npm run test-jest && concurrently \"npm run storybook-test\" \"wait-on tcp:9999 -d 30000 --tcpTimeout 30000 && TEST_HOST_STORYBOOK_URL=http://localhost:9999 jest --config=jest.ui.config.js && testcafe -c 4 'chrome:headless' -q ./**/ui.test.js\"",
    "new": "node scripts/create-a-m-o/index.js",
    "deploy-branch": "npm run build-storybook && SOURCE_BRANCH_NAME=\"$(git rev-parse --abbrev-ref HEAD)\" bash scripts/branch-deployment.sh && SOURCE_BRANCH_NAME=\"$(git rev-parse --abbrev-ref HEAD)\" bash scripts/branch-deployment-notifier.sh",
    "link-components": "sh scripts/npm-link-components.sh",
    "check-for-uncommitted-files": "git status && git diff-index --quiet HEAD --"
  },
  "dependencies": {
    "@skatejs/val": "^0.4.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "lit-element": "^2.2.0",
    "lit-html": "^1.1.0"
  },
  "devDependencies": {
    "@axa-ch/materials": "latest",
    "@axa-ch/patterns-library-polyfill": "latest",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/components": "^6.2.9",
    "@storybook/html": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "chalk": "^2.4.2",
    "clipboardy": "^2.0.0",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^2.6.5",
    "cp": "^0.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv-extended": "^2.3.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
