{
  "name": "@axa-ch/patterns-library",
  "version": "2.0.0-beta",
  "description": "AXA Switzerland Web Patterns",
  "main": "stack/server.js",
  "scripts": {
    "test": "parallelshell 'npm run test-eslint' 'npm run test-stylelint'",
    "test-eslint": "eslint src/",
    "test-stylelint": "stylelint 'src/**/*.scss'",
    "start": "npm run serve-build-prod",
    "new": "node stack/tasks/create-a-m-o.js",
    "server": "node stack/server.js",
    "postinstall": "npm rebuild node-sass",
    "watch": "npm-watch build-dev",
    "serve": "parallelshell \"npm run server DEV\" \"npm run watch\"",
    "bundle-dev": "node stack/tasks/bundle.js DEV && node stack/tasks/copy-polyfills.js DEV",
    "bundle-prod": "node stack/tasks/bundle.js PROD && node stack/tasks/copy-polyfills.js PROD",
    "serve-build-prod": "parallelshell \"npm run server PROD\" \"npm run build\"",
    "build-css-dev": "node-sass src/index.scss -o .tmp/",
    "build-css-prod": "node-sass src/index.scss -o dist/",
    "build-js-dev": "npm run bundle-dev",
    "build-js-prod": "npm run bundle-prod",
    "build-icons-dev": "node stack/tasks/icons.js DEV",
    "build-icons-prod": "node stack/tasks/icons.js PROD",
    "build-dev": "node stack/tasks/cleanup.js DEV && npm run build-css-dev && node stack/tasks/build.html.js DEV && node stack/tasks/build-examples.js DEV && npm run build-js-dev && npm run build-icons-dev",
    "build": "node stack/tasks/cleanup.js PROD && node stack/tasks/build.html.js PROD && node stack/tasks/build-examples.js PROD && npm run build-css-prod && npm run build-js-prod && npm run build-icons-prod",
    "bump-major": "npm run test && npmversion --increment major",
    "bump-minor": "npm run test && npmversion --increment minor",
    "bump-patch": "npm run test && npmversion --increment patch",
    "bump-major-beta": "npm run test && npmversion --increment major --preid beta --force-preid"
  },
  "watch": {
    "build-dev": {
      "patterns": [
        "src"
      ],
      "extensions": "js,html,scss,svg",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axa-ch/patterns-library"
  },
  "keywords": [
    "axa"
  ],
  "engines": {
    "node": ">=8.8.1",
    "npm": "~5.4.2"
  },
  "author": "luca.mele@axa-winterthur.ch",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "~1.6.1",
    "compression": "^1.7.1",
    "cpx": "~1.5.0",
    "cross-env": "^5.1.1",
    "del": "~3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "~4.16.2",
    "glob": "^7.1.2",
    "mkdirp": "~0.5.1",
    "node-dir": "^0.1.17",
    "node-sass": "^4.5.3",
    "node-syntaxhighlighter": "^0.8.1",
    "npm-watch": "~0.3.0",
    "npmversion": "~1.4.0",
    "outdent": "^0.4.1",
    "parallelshell": "~3.0.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-uglify": "^2.0.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "svg-sprite": "^1.3.7",
    "vinyl": "^2.1.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.18",
    "bel": "^5.1.5",
    "classnames": "^2.2.5",
    "core-js": "~2.5.1",
    "innersvg-polyfill": "0.0.2"
  }
}
