    "mv": "^2.1.1",
    "outdent": "^0.7.0",
    "playwright-chromium": "^1.9.2",
    "postcss": "^7.0.26",
    "postcss-strip-font-face": "^1.0.0",
    "prettier": "1.19.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-syntax-highlighter": "^12.2.1",
    "rimraf": "^2.6.3",
    "rollup": "^2.21.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-sass": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sass": "^1.33.0",
    "sass-loader": "^7.1.0",
    "svgo": "^1.2.0",
    "testcafe": "1.9.2",
    "testcafe-browser-provider-browserstack": "1.12.0",
    "to-string-loader": "^1.1.5",
    "wait-on": "^6.0.0"
  },
  "repository": {
    "type": "git",
