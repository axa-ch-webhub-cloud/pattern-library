{
  "name": "@axa-ch/patterns-library",
  "version": "2.0.0",
  "description": "[Check out our Contribution File](https://github.com/axa-ch/patterns-library/blob/develop-v2/CONTRIBUTION.md#rules-and-lintings)",
  "module": "lib",
  "scripts": {
    "postinstall": "npm-recursive-install --skip-root",
    "build": "npm run clean-build && npm run build-materials && cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "build-materials": "npm run build-icons && node scripts/build.materials.js",
    "build-icons": "npm run clean-materials && svgo -f src/components/00-materials/icons -o src/components/00-materials/icons/.tmp && node scripts/build.icon-set.js && npm run clean-tmp-icons",
    "clean-build": "rimraf src/components/*/*/lib && rimraf src/components/*/*/dist",
    "clean-materials": "npm run clean-build && npm run clean-tmp-icons",
    "clean-lib-materials": "rimraf src/components/00-materials/lib/**",
    "clean-tmp-icons": "rimraf src/components/00-materials/icons/.tmp",
    "release": "npm whoami && npm run test && npm run build && lerna publish",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -c .storybook -s src/static",
    "build-storybook": "build-storybook",
    "test": "npm run test-lint && node ./scripts/build.test.js && npm run test-chrome",
    "test-lint": "eslint src/",
    "test-chrome": "sh scripts/testcafe-chrome.sh",
    "test-ie": "sh scripts/testcafe-ie.sh",
    "test-jest": "node scripts/test.unit.js --env=jsdom"
  },
  "dependencies": {
    "@skatejs/val": "^0.4.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@storybook/html": "^5.0.8",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "breakpoint-sass": "^2.7.1",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv-extended": "^2.3.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "lerna": "^3.13.1",
    "markdown-loader": "^5.0.0",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.11.0",
    "outdent": "^0.7.0",
    "postcss": "^7.0.14",
    "postcss-strip-font-face": "^1.0.0",
    "prettier": "1.17.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "recursive-install": "^1.4.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sass": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "svgo": "^1.2.0",
    "testcafe": "^1.1.0",
    "testcafe-browser-provider-browserstack": "^1.8.0",
    "to-string-loader": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axa-ch/patterns-library.git"
  },
  "author": "Pattern Warriors",
  "license": "Copyright 2019 AXA Versicherungen AG",
  "bugs": {
    "url": "https://github.com/axa-ch/patterns-library/issues"
  },
  "homepage": "https://github.com/axa-ch/patterns-library#readme"
}
