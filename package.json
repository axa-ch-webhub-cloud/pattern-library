{
  "name": "@axa-ch/patterns-library",
  "version": "2.0.0",
  "description": "[Check out our Contribution File](https://github.com/axa-ch/patterns-library/blob/develop-v2/CONTRIBUTION.md#rules-and-lintings)",
  "module": "lib",
  "scripts": {
    "build": "npm run clean-lib && cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "clean-lib": "rimraf src/**/lib",
    "release": "npm whoami && npm run test && npm run build && lerna publish",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./src/static",
    "test": "eslint src/",
    "cy": "npx cypress run",
    "tc": "testcafe \"chrome:headless\" testcafe/",
    "bs": "testcafe \"browserstack:Chrome@53.0:Windows 10\" testcafe/",
    "tes": "npm run storybook & wait-on http://localhost:9001 && npm run cy"
  },
  "dependencies": {
    "@skatejs/val": "^0.4.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "classnames": "^2.2.6",
    "dotenv-extended": "^2.3.0",
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-options": "^4.1.13",
    "@storybook/html": "^4.1.12",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cypress": "^3.1.5",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "lerna": "^3.13.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-strip-font-face": "^1.0.0",
    "prettier": "1.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.3.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sass": "^1.1.0",
    "sass-loader": "^7.1.0",
    "testcafe": "^1.1.0",
    "testcafe-browser-provider-browserstack": "^1.8.0",
    "to-string-loader": "^1.1.5",
    "wait-on": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axa-ch/patterns-library.git"
  },
