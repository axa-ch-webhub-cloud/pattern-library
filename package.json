{
  "name": "@axa-ch/patterns-library",
  "version": "2.0.0",
  "description": "[Check out our Contribution File](https://github.com/axa-ch/patterns-library/blob/develop/CONTRIBUTION.md#rules-and-lintings)",
  "module": "lib",
  "scripts": {
    "build": "NODE_ENV=production npm run build-components",
    "watch": "npm run watch-components",
    "build-components": "lerna run build --parallel",
    "watch-components": "lerna run watch --parallel",
    "setup-npm-links": "node scripts/setup-npm-link.js",
    "postinstall": "lerna bootstrap --hoist && npm run build-components",
    "release": "npm whoami && lerna bootstrap --hoist && npm run build && npm run test && lerna publish",
    "prerelease": "npm whoami && lerna bootstrap --hoist && npm run build && lerna publish prerelease",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -c .storybook -s src/static",
    "build-storybook": "build-storybook",
    "test": "npm run test-lint && prettier --check ./src/**/* && node ./scripts/build.test.js && npm run test-jest && npm run test-chrome",
    "test-lint": "eslint src/",
    "test-chrome": "sh scripts/testcafe-chrome.sh",
    "test-ie": "sh scripts/testcafe-ie.sh",
    "test-jest": "jest --env=jsdom",
    "test-jest-watch": "jest --env=jsdom --watch -o",
    "test-ui": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 testcafe chrome -c 4 ./**/ui.test.js",
    "test-ui-hl": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 testcafe 'chrome:headless' -c 4 ./**/ui.test.js",
    "new": "node scripts/create-a-m-o/index.js"
  },
  "dependencies": {
    "@skatejs/val": "^0.4.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "lit-element": "^2.2.0",
    "lit-html": "^1.1.0"
  },
  "devDependencies": {
    "@axa-ch/patterns-library-polyfill": "^1.4.12",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/html": "^5.0.8",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "chalk": "^2.4.2",
    "clipboardy": "^2.0.0",
    "core-js": "^2.6.5",
    "cp": "^0.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv-extended": "^2.3.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "lerna": "^3.13.4",
    "markdown-loader": "^5.0.0",
    "mkdirp": "^0.5.1",
    "mutationobserver-shim": "^0.3.3",
    "mv": "^2.1.1",
    "node-sass": "^4.12.0",
    "outdent": "^0.7.0",
    "postcss": "^7.0.14",
    "postcss-strip-font-face": "^1.0.0",
    "prettier": "1.17.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sass": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "storybook-readme": "^5.0.4-alpha.2",
    "svgo": "^1.2.0",
    "testcafe": "^1.1.0",
